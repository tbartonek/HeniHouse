<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_black_3"
    android:baselineAligned="false"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="horizontal" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="@dimen/widthMenu"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:paddingTop="10dp"
        android:scrollbars="none" >

        <RelativeLayout
            android:id="@+id/RelativeLayout1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center|top"
            android:orientation="vertical" >

            <ImageButton
                android:id="@+id/ibLock"
                android:layout_width="@dimen/bMiddle"
                android:layout_height="@dimen/bMiddle"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:adjustViewBounds="false"
                android:background="@android:color/transparent"
                android:scaleType="centerInside"
                android:src="@drawable/lock_icon_128" />

            <TextView
                android:id="@+id/TextView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ibLock"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:paddingBottom="@dimen/bGapSmall"
                android:text="@string/bAlarm"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/bTextSmall"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/ibDevices"
                android:layout_width="@dimen/bMiddle"
                android:layout_height="@dimen/bMiddle"
                android:layout_below="@+id/TextView1"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:adjustViewBounds="false"
                android:background="@android:color/transparent"
                android:scaleType="centerInside"
                android:src="@drawable/device_icon_128" />

            <TextView
                android:id="@+id/TextView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ibDevices"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:paddingBottom="@dimen/bGapSmall"
                android:text="@string/bDevices"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/bTextSmall"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/ibHeaters"
                android:layout_width="@dimen/bMiddle"
                android:layout_height="@dimen/bMiddle"
                android:layout_below="@+id/TextView5"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:adjustViewBounds="false"
                android:background="@android:color/transparent"
                android:scaleType="centerInside"
                android:src="@drawable/climatic_icon_128" />

            <TextView
                android:id="@+id/TextView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ibHeaters"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:paddingBottom="@dimen/bGapSmall"
                android:text="@string/bHeaters"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/bTextSmall"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/ibWeathers"
                android:layout_width="@dimen/bMiddle"
                android:layout_height="@dimen/bMiddle"
                android:layout_below="@+id/TextView7"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:adjustViewBounds="false"
                android:background="@android:color/transparent"
                android:scaleType="centerInside"
                android:src="@drawable/weather_icon_128" />

            <TextView
                android:id="@+id/TextView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ibWeathers"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:paddingBottom="@dimen/bGapSmall"
                android:text="@string/bWeathers"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/bTextSmall"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/ibComunicaton"
                android:layout_width="@dimen/bMiddle"
                android:layout_height="@dimen/bMiddle"
                android:layout_below="@+id/TextView8"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:adjustViewBounds="false"
                android:background="@android:color/transparent"
                android:scaleType="centerInside"
                android:src="@drawable/comunication_icon_128" />

            <TextView
                android:id="@+id/TextView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ibComunicaton"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:paddingBottom="@dimen/bGapSmall"
                android:text="@string/bComunication"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/bTextSmall"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/ibCameras"
                android:layout_width="@dimen/bMiddle"
                android:layout_height="@dimen/bMiddle"
                android:layout_below="@+id/TextView2"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:adjustViewBounds="false"
                android:background="@android:color/transparent"
                android:scaleType="centerInside"
                android:src="@drawable/camera_icon_128" />

            <TextView
                android:id="@+id/TextView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ibCameras"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:paddingBottom="@dimen/bGapSmall"
                android:text="@string/bCameras"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/bTextSmall"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/ibMultimedia"
                android:layout_width="@dimen/bMiddle"
                android:layout_height="@dimen/bMiddle"
                android:layout_below="@+id/TextView6"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:adjustViewBounds="false"
                android:background="@android:color/transparent"
                android:scaleType="centerInside"
                android:src="@drawable/movie_icon_128" />

            <TextView
                android:id="@+id/TextView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ibMultimedia"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:paddingBottom="@dimen/bGapSmall"
                android:text="@string/bMultimedia"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/bTextSmall"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/ibSettings"
                android:layout_width="@dimen/bMiddle"
                android:layout_height="@dimen/bMiddle"
                android:layout_below="@+id/TextView3"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:adjustViewBounds="false"
                android:background="@android:color/transparent"
                android:scaleType="centerInside"
                android:src="@drawable/services_icon_128" />

            <TextView
                android:id="@+id/TextView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/ibSettings"
                android:layout_centerHorizontal="true"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:text="@string/bSettings"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/bTextSmall"
                android:textStyle="bold" />
        </RelativeLayout>
    </ScrollView>

    <LinearLayout
        android:layout_width="460dp"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingTop="10dp" >

        <TextView
            android:id="@+id/tvAlarmTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/alarm_title"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="#FFFFFF"
            android:textSize="@dimen/bTextLarge"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/alarmLayout"
            android:layout_width="fill_parent"
            android:layout_height="320dp"
            android:background="#000"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/alarmNotice"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:maxLines="50"
                android:paddingLeft="5dp"
                android:scrollbars="vertical"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/bTextLittle"
                android:textStyle="bold" />

            <ScrollView
                android:id="@+id/alarmSettingLayout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:scrollbars="none"
                android:visibility="gone" >

                <TableLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top"
                    android:gravity="top"
                    android:orientation="vertical"
                    android:scrollbars="vertical" >

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvMaxTryPassword_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/alarm_MaxTryPassword"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/tvMaxTryValue"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:background="@android:color/transparent"
                            android:inputType="number"
                            android:paddingLeft="10dp"
							android:maxLength="2"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />
                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvAlertTime_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/alarm_AlertTime"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/tvAlertTimeValue"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:background="@android:color/transparent"
                            android:inputType="number"
                            android:maxLength="2"
                            android:paddingLeft="10dp"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />
                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvStartTime_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/alarm_StartTime"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/tvStartTimeValue"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:background="@android:color/transparent"
                            android:inputType="number"
                            android:maxLength="2"
                            android:paddingLeft="10dp"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />
                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvPasswordChange_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|left"
                            android:text="@string/alarm_PasswordChange"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />

                        <CheckBox
                            android:id="@+id/cbPassword"
                            android:layout_gravity="right"
                            android:scaleX="1"
                            android:scaleY="1" />

                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvPassword_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/alarm_Password"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/tvPasswordValue"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:background="#FFFFFF"
                            android:inputType="numberPassword"
                            android:maxLength="6"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#000"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />
                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvPasswordNew1_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/alarm_PasswordNew1"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/tvPasswordNewValue1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#FFFFFF"
                            android:inputType="numberPassword"
                            android:maxLength="6"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#000"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />
                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvPasswordNew2_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/alarm_PasswordNew2"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/tvPasswordNewValue2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#FFFFFF"
                            android:inputType="numberPassword"
                            android:maxLength="6"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#000"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />
                    </TableRow>
                    
                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvServisPasswordChange_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|left"
                            android:layout_marginLeft="5dp"
                            android:text="@string/alarm_ServisPasswordChange"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />

                        <CheckBox
                            android:id="@+id/cbServisPassword"
                            android:layout_gravity="right" />

                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvServisPassword_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/alarm_Password"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/tvServisPasswordValue"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:background="#FFFFFF"
                            android:inputType="numberPassword"
                            android:maxLength="6"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#000"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />
                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:orientation="horizontal" >

                        <TextView
                            android:id="@+id/tvServisPasswordNew1_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/alarm_PasswordNew1"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/tvServisPasswordNewValue1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#FFFFFF"
                            android:inputType="numberPassword"
                            android:maxLength="6"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#000"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />
                    </TableRow>

                    <TableRow
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:orientation="horizontal"
                        android:paddingBottom="100dp" >

                        <TextView
                            android:id="@+id/tvServisPasswordNew2_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/alarm_PasswordNew2"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#FFFFFF"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />

                        <EditText
                            android:id="@+id/tvServisPasswordNewValue2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:background="#FFFFFF"
                            android:inputType="numberPassword"
                            android:maxLength="6"
                            android:textAppearance="?android:attr/textAppearanceLarge"
                            android:textColor="#000"
                            android:textSize="@dimen/bTextLarge"
                            android:textStyle="bold" />
                    </TableRow>
                </TableLayout>
            </ScrollView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="bottom"
            android:orientation="horizontal"
            android:paddingBottom="10dp"
            android:paddingTop="10dp" >

            <ImageButton
                android:id="@+id/ibSettingsAlarm"
                android:layout_width="@dimen/bKeypad"
                android:layout_height="@dimen/bKeypad"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:onClick="buttonSetting"
                android:scaleType="centerInside"
                android:src="@drawable/settings" />

            <TextView
                android:id="@+id/tvSettings"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:paddingLeft="10dp"
                android:paddingRight="0dp"
                android:text="@string/alarm_setting"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/bTextSmall"
                android:textStyle="bold" />

            <ImageButton
                android:id="@+id/ibSettingsCancel"
                android:layout_width="@dimen/bKeypad"
                android:layout_height="@dimen/bKeypad"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:onClick="buttonSettingCancel"
                android:scaleType="centerInside"
                android:src="@drawable/cancel"
                android:visibility="gone" />

            <TextView
                android:id="@+id/tvSettingsCancel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|right"
                android:paddingLeft="10dp"
                android:text="@string/alarm_cancel"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:textColor="#FFFFFF"
                android:textSize="@dimen/bTextSmall"
                android:textStyle="bold"
                android:visibility="gone" />

            <ImageButton
                android:id="@+id/ibStateAlarm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:background="@android:color/transparent"
                android:onClick="buttonLock"
                android:scaleType="centerInside"
                android:src="@drawable/unlock" />
        </LinearLayout>
    </LinearLayout>

    <TableLayout
        android:id="@+id/layoutNumbers"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center|right" >

        <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/bKeypadGap"
            android:gravity="right" >

            <ImageButton
                android:id="@+id/ibOne"
                android:layout_width="@dimen/bKeypad"
                android:layout_height="@dimen/bKeypad"
                android:background="@android:color/transparent"
                android:onClick="buttonNumber"
                android:scaleType="centerInside"
                android:src="@drawable/one" />

            <ImageButton
                android:id="@+id/ibTwo"
                android:layout_width="@dimen/bKeypad"
                android:layout_height="@dimen/bKeypad"
                android:background="@android:color/transparent"
                android:onClick="buttonNumber"
                android:scaleType="centerInside"
                android:src="@drawable/two" />

            <ImageButton
                android:id="@+id/ibThree"
                android:layout_width="@dimen/bKeypad"
                android:layout_height="@dimen/bKeypad"
                android:background="@android:color/transparent"
                android:onClick="buttonNumber"
                android:scaleType="centerInside"
                android:src="@drawable/three" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="@dimen/bKeypadGap"
            android:gravity="right" >

            <ImageButton
                android:id="@+id/ibFour"
                android:layout_width="@dimen/bKeypad"
                android:layout_height="@dimen/bKeypad"
                android:background="@android:color/transparent"
                android:onClick="buttonNumber"
                android:scaleType="centerInside"
                android:src="@drawable/four" />

            <ImageButton
                android:id="@+id/ibFive"
                android:layout_width="@dimen/bKeypad"
                android:layout_height="@dimen/bKeypad"
                android:background="@android:color/transparent"
                android:onClick="buttonNumber"
                android:scaleType="centerInside"
                android:src="@drawable/five" />

            <ImageButton
                android:id="@+id/ibSix"
                android:layout_width="@dimen/bKeypad"
                android:layout_height="@dimen/bKeypad"
                android:background="@android:color/transparent"
                android:onClick="buttonNumber"
                android:scaleType="centerInside"
                android:src="@drawable/six" />
